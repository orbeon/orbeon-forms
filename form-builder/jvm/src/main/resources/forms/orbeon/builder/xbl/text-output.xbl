            <display-name lang="it" todo="true">Testo statico</display-name>
            <display-name lang="nl" todo="true">Tekstuitvoer</display-name>
            <display-name lang="sv" todo="true">Beräknat värde</display-name>
            <display-name lang="pt" todo="true">Saída de Texto</display-name>
            <appearance-display-name lang="en">With border</appearance-display-name>
            <appearance-display-name lang="es" todo="true">[With border]</appearance-display-name>
            <appearance-display-name lang="fr">Avec bord</appearance-display-name>
            <appearance-display-name lang="ru" todo="true">[With border]</appearance-display-name>
            <appearance-display-name lang="de" todo="true">[With border]</appearance-display-name>
            <appearance-display-name lang="it" todo="true">[With border]</appearance-display-name>
            <appearance-display-name lang="nl" todo="true">[With border]</appearance-display-name>
