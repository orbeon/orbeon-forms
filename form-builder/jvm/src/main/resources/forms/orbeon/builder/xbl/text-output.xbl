<xbl:xbl xmlns:xf="http://www.w3.org/2002/xforms"
         xmlns:xbl="http://www.w3.org/ns/xbl">

    <xbl:binding id="fb-output" element="xf|output">
        <metadata xmlns="http://orbeon.org/oxf/xml/form-builder">
            <display-name lang="en">Calculated Value</display-name>
            <display-name lang="es">Valor Calculado</display-name>
            <display-name lang="fr">Valeur calculée</display-name>
            <display-name lang="ru" todo="true">Неизменяемый текст</display-name>
            <display-name lang="de" todo="true">Textausgabe</display-name>
            <display-name lang="it" todo="true">Testo statico</display-name>
            <display-name lang="nl" todo="true">Tekstuitvoer</display-name>
            <display-name lang="sv">Beräknat värde</display-name>
            <display-name lang="pt" todo="true">Saída de Texto</display-name>
            <appearance-display-name lang="en">Use default</appearance-display-name>
            <appearance-display-name lang="es" todo="true">[Use default]</appearance-display-name>
            <appearance-display-name lang="fr">Défaut</appearance-display-name>
            <appearance-display-name lang="ru" todo="true">[Use default]</appearance-display-name>
            <appearance-display-name lang="de" todo="true">[Use default]</appearance-display-name>
            <appearance-display-name lang="it" todo="true">[Use default]</appearance-display-name>
            <appearance-display-name lang="nl" todo="true">[Use default]</appearance-display-name>
            <appearance-display-name lang="sv" todo="true">[Use default]</appearance-display-name>
            <appearance-display-name lang="pt" todo="true">[Use default]</appearance-display-name>
            <icon>
                <icon-class>fa fa-fw fa-calculator</icon-class>
            </icon>
            <templates>
                <view>
                    <xf:output id="" ref="" xmlns="">
                        <xf:label ref=""/>
                        <xf:hint ref=""/>
                        <xf:help ref=""/>
                        <xf:alert ref=""/>
                    </xf:output>
                </view>
            </templates>
        </metadata>
    </xbl:binding>

    <xbl:binding id="fb-output-full" element="xf|output[appearance ~= full]">
        <metadata xmlns="http://orbeon.org/oxf/xml/form-builder">
