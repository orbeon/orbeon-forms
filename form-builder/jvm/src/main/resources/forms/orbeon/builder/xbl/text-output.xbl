                        <xf:label ref=""/>
                        <xf:hint ref=""/>
                        <xf:help ref=""/>
                        <xf:alert ref=""/>
                    </xf:output>
                </view>
            </templates>
        </metadata>
    </xbl:binding>

    <xbl:binding id="fb-output-minimal" element="xf|output[appearance ~= minimal]">
        <metadata xmlns="http://orbeon.org/oxf/xml/form-builder">
