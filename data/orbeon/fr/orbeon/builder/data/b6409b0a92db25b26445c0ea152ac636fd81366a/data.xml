<xh:html xmlns:xh="http://www.w3.org/1999/xhtml" xmlns:xf="http://www.w3.org/2002/xforms"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:ev="http://www.w3.org/2001/xml-events"
         xmlns:xi="http://www.w3.org/2001/XInclude"
         xmlns:xxi="http://orbeon.org/oxf/xml/xinclude"
         xmlns:xxf="http://orbeon.org/oxf/xml/xforms"
         xmlns:map="http://www.w3.org/2005/xpath-functions/map"
         xmlns:array="http://www.w3.org/2005/xpath-functions/array"
         xmlns:math="http://www.w3.org/2005/xpath-functions/math"
         xmlns:exf="http://www.exforms.org/exf/1-0"
         xmlns:fr="http://orbeon.org/oxf/xml/form-runner"
         xmlns:saxon="http://saxon.sf.net/"
         xmlns:sql="http://orbeon.org/oxf/xml/sql"
         xmlns:soap="http://schemas.xmlsoap.org/soap/envelope/"
         xmlns:fb="http://orbeon.org/oxf/xml/form-builder">
    <xh:head>
        <xh:title>Global Library</xh:title>
        <xf:model id="fr-form-model" xxf:expose-xpath-types="true" xxf:analysis.calculate="true">

            <!-- Main instance -->
            <xf:instance id="fr-form-instance" xxf:exclude-result-prefixes="#all" xxf:index="id">
                <form>
                    <address-us>
                        <grid-3>
                            <us-address-street-1/>
                            <us-address-city/>


                            <us-address-street-2/>
                            <us-address-state/>
                            <us-address-zip/>


                            <us-address-urbanization/>

                        </grid-3>
                    </address-us>
                    <address-uk>
                        <grid-2>
                            <uk-address-line-1/>
                            <uk-address-town/>
                            <uk-address-line-2/>
                            <uk-address-postcode/>
                            <uk-address-locality/>


                        </grid-2>
                    </address-uk>
                    <contact-information>
                        <grid-5>
                            <contact-information-first-name/>
                            <contact-information-last-name/>
                            <contact-information-phone/>
                            <contact-information-email/>
                            <control-4/>
                        </grid-5>
                    </contact-information>
                </form>
            </xf:instance>

            <!-- Bindings -->
            <xf:bind id="fr-form-binds" ref="instance('fr-form-instance')">
                <xf:bind id="address-us-bind" ref="address-us" name="address-us">
                    <xf:bind id="grid-3-bind" ref="grid-3" name="grid-3">
                        <xf:bind id="us-address-street-1-bind" ref="us-address-street-1"
                                 name="us-address-street-1"
                                 xxf:whitespace="trim"/>
                        <xf:bind id="us-address-street-2-bind" ref="us-address-street-2"
                                 name="us-address-street-2"
                                 xxf:whitespace="trim"/>



                        <xf:bind id="us-address-urbanization-bind" ref="us-address-urbanization"
                                 name="us-address-urbanization"
                                 xxf:whitespace="trim"
                                 relevant="$us-address-state = 'PR'"/>
                        <xf:bind id="us-address-zip-bind" ref="us-address-zip" name="us-address-zip"
                                 xxf:whitespace="trim"
                                 required="true()"
                                 constraint="matches(., '^[\d]{5}(-[\d]{4})?$')"/>
                        <xf:bind id="us-address-state-bind" ref="us-address-state" name="us-address-state"/>
                        <xf:bind id="us-address-city-bind" ref="us-address-city" name="us-address-city"
                                 xxf:whitespace="trim"
                                 required="true()"/>

                    </xf:bind>
                </xf:bind>
                <xf:bind id="address-uk-bind" ref="address-uk" name="address-uk">
                    <xf:bind id="grid-2-bind" ref="grid-2" name="grid-2">
                        <xf:bind id="uk-address-line-1-bind" ref="uk-address-line-1" name="uk-address-line-1"
                                 xxf:whitespace="trim"
                                 required="true()"
                                 constraint="xxf:max-length(255)"/>
                        <xf:bind id="uk-address-line-2-bind" ref="uk-address-line-2" name="uk-address-line-2"
                                 xxf:whitespace="trim"
                                 constraint="xxf:max-length(255)"/>


                        <xf:bind id="uk-address-locality-bind" ref="uk-address-locality"
                                 name="uk-address-locality"
                                 xxf:whitespace="trim"
                                 constraint="xxf:max-length(35)"/>
                        <xf:bind id="uk-address-town-bind" ref="uk-address-town" name="uk-address-town"
                                 xxf:whitespace="trim"
                                 required="true()"
                                 constraint="xxf:max-length(35)"
                                 calculate="upper-case(.)"
                                 readonly="false()"/>
                        <xf:bind id="uk-address-postcode-bind" ref="uk-address-postcode"
                                 name="uk-address-postcode"
                                 xxf:whitespace="trim"
                                 required="true()"
                                 constraint="xxf:max-length(8)"
                                 calculate="upper-case(.)"
                                 readonly="false()"/>
                    </xf:bind>
                </xf:bind>
                <xf:bind id="contact-information-bind" ref="contact-information"
                         name="contact-information">
                    <xf:bind id="grid-5-bind" ref="grid-5" name="grid-5">
                        <xf:bind id="contact-information-first-name-bind" ref="contact-information-first-name"
                                 name="contact-information-first-name"
                                 xxf:whitespace="trim"
                                 required="true()"/>
                        <xf:bind id="contact-information-phone-bind" ref="contact-information-phone"
                                 name="contact-information-phone"
                                 xxf:whitespace="trim"
                                 required="true()"/>
                        <xf:bind id="contact-information-email-bind" ref="contact-information-email"
                                 name="contact-information-email"
                                 constraint=". = '' or matches(., '^\d{10}$')"/>
                        <xf:bind id="control-4-bind" ref="control-4" name="control-4" type="xf:email"
                                 xxf:whitespace="trim"
                                 required="true()"/>
                        <xf:bind id="contact-information-last-name-bind" ref="contact-information-last-name"
                                 name="contact-information-last-name"
                                 xxf:whitespace="trim"/>
                    </xf:bind>
                </xf:bind>
            </xf:bind>

            <!-- Metadata -->
            <xf:instance id="fr-form-metadata" xxf:readonly="true" xxf:exclude-result-prefixes="#all">
                <metadata>
                    <application-name>_</application-name>
                    <form-name>library</form-name>
                    <title xml:lang="en">Global Library</title>
                    <description xml:lang="en">This is the Orbeon Forms global library of section templates. It can be used by any form.</description>
                    <created-with-version>2024.1-SNAPSHOT PE</created-with-version>
                    <updated-with-version>2024.1-SNAPSHOT PE</updated-with-version>
                    <grid-tab-order>default</grid-tab-order>
                    <library-versions>
                        <orbeon>1</orbeon>
                    </library-versions>


                    <email>
                        <templates>
                            <template name="default">
                                <headers/>
                                <exclude-from-all-control-values/>
                            </template>
                        </templates>
                        <parameters/>
                    </email>
                </metadata>
            </xf:instance>

            <!-- Attachments -->
            <xf:instance id="fr-form-attachments" xxf:exclude-result-prefixes="#all">
                <attachments/>
            </xf:instance>

            <!-- All form resources -->
            <xf:instance xxf:readonly="true" id="fr-form-resources" xxf:exclude-result-prefixes="#all">
                <resources>
                    <resource xml:lang="en">
                        <contact-information-first-name>
                            <label>First Name</label>
                            <hint/>
                        </contact-information-first-name>
                        <contact-information-last-name>
                            <label>Middle initial</label>
                            <hint/>
                        </contact-information-last-name>
                        <contact-information-phone>
                            <label>Last Name</label>
                            <hint/>
                        </contact-information-phone>
                        <contact-information-email>
                            <label>Phone number</label>
                            <hint/>
                        </contact-information-email>
                        <control-4>
                            <label>Email</label>
                            <hint/>
                        </control-4>
                        <us-address-street-1>
                            <label>Street address</label>
                            <hint/>
                        </us-address-street-1>
                        <us-address-city>
                            <label>City</label>
                            <hint/>
                        </us-address-city>


                        <us-address-street-2>
                            <label>Street address line 2</label>
                            <hint/>
                        </us-address-street-2>
                        <us-address-state>
                            <label>State, territory, or military post</label>
                            <hint/>
                        </us-address-state>
                        <us-address-zip>
                            <label>Zip code</label>
                            <hint/>
                        </us-address-zip>


                        <us-address-urbanization>
                            <label>Urbanization</label>
                            <hint/>
                        </us-address-urbanization>

                        <address-us>
                            <label>US address</label>

                        </address-us>
                        <address-uk>
                            <label>UK Address</label>
                        </address-uk>
                        <uk-address-line-1>
                            <label>Address line 1</label>
                            <hint/>
                        </uk-address-line-1>
                        <uk-address-town>
                            <label>Town</label>
                            <hint/>
                        </uk-address-town>
                        <uk-address-line-2>
                            <label>Address line 2</label>
                            <hint/>
                        </uk-address-line-2>
                        <uk-address-postcode>
                            <label>Postcode</label>
                            <hint/>
                        </uk-address-postcode>


                        <uk-address-locality>
                            <label>Locality</label>
                            <hint/>
                        </uk-address-locality>
                        <contact-information>
                            <label>Contact information</label>
                        </contact-information>
                        <grid-3/>
                        <grid-2/>


                    </resource>
                </resources>
            </xf:instance>

        </xf:model>
    </xh:head>
    <xh:body>
        <fr:view>
            <fr:body xmlns:xbl="http://www.w3.org/ns/xbl" xmlns:p="http://www.orbeon.com/oxf/pipeline"
                     xmlns:oxf="http://www.orbeon.com/oxf/processors">
                <fr:section id="address-us-section" bind="address-us-bind">
                    <xf:label ref="$form-resources/address-us/label"/>


                    <fr:grid id="grid-3-grid" bind="grid-3-bind" tab-order="columns">
                        <fr:c x="1" y="1" w="6">
                            <xf:input id="us-address-street-1-control" bind="us-address-street-1-bind"
                                      autocomplete="address-line1">
                                <xf:label ref="$form-resources/us-address-street-1/label"/>
                                <xf:hint ref="$form-resources/us-address-street-1/hint"/>
                                <xf:alert ref="$fr-resources/detail/labels/alert"/>


                            </xf:input>
                        </fr:c>
                        <fr:c x="7" y="1" w="6">
                            <xf:input id="us-address-city-control" bind="us-address-city-bind"
                                      autocomplete="address-line3">
                                <xf:label ref="$form-resources/us-address-city/label"/>
                                <xf:hint ref="$form-resources/us-address-city/hint"/>
                                <xf:alert ref="$fr-resources/detail/labels/alert"/>


                            </xf:input>

                        </fr:c>

                        <fr:c x="1" y="2" w="6">
                            <xf:input id="us-address-street-2-control" bind="us-address-street-2-bind"
                                      autocomplete="address-line2">
                                <xf:label ref="$form-resources/us-address-street-2/label"/>
                                <xf:hint ref="$form-resources/us-address-street-2/hint"/>
                                <xf:alert ref="$fr-resources/detail/labels/alert"/>


                            </xf:input>
                        </fr:c>
                        <fr:c x="7" y="2" w="4">
                            <fr:us-state xmlns="http://orbeon.org/oxf/xml/form-builder"
                                         xmlns:xxbl="http://orbeon.org/oxf/xml/xbl"
                                         id="us-address-state-control"
                                         bind="us-address-state-bind">
                                <xf:label ref="$form-resources/us-address-state/label"/>
                                <xf:hint ref="$form-resources/us-address-state/hint"/>
                                <xf:alert ref="$fr-resources/detail/labels/alert"/>


                            </fr:us-state>


                        </fr:c>
                        <fr:c x="11" y="2" w="2" h="1">
                            <xf:input id="us-address-zip-control" bind="us-address-zip-bind"
                                      autocomplete="postal-code">
                                <xf:label ref="$form-resources/us-address-zip/label"/>
                                <xf:hint ref="$form-resources/us-address-zip/hint"/>
                                <xf:alert ref="$fr-resources/detail/labels/alert"/>


                            </xf:input>
                        </fr:c>
                        <fr:c x="1" y="3" w="6">

                        </fr:c>
                        <fr:c x="7" y="3" w="6">
                            <xf:input id="us-address-urbanization-control" bind="us-address-urbanization-bind">
                                <xf:label ref="$form-resources/us-address-urbanization/label"/>
                                <xf:hint ref="$form-resources/us-address-urbanization/hint"/>
                                <xf:alert ref="$fr-resources/detail/labels/alert"/>


                            </xf:input>

                        </fr:c>
                    </fr:grid>
                </fr:section>
                <fr:section id="address-uk-section" bind="address-uk-bind">
                    <xf:label ref="$form-resources/address-uk/label"/>
                    <fr:grid id="grid-2-grid" bind="grid-2-bind" tab-order="columns">
                        <fr:c x="1" y="1" w="6">
                            <xf:input id="uk-address-line-1-control" bind="uk-address-line-1-bind">
                                <xf:label ref="$form-resources/uk-address-line-1/label"/>
                                <xf:hint ref="$form-resources/uk-address-line-1/hint"/>
                                <xf:alert ref="$fr-resources/detail/labels/alert"/>


                            </xf:input>
                        </fr:c>
                        <fr:c x="7" y="1" w="6">
                            <xf:input id="uk-address-town-control" bind="uk-address-town-bind">
                                <xf:label ref="$form-resources/uk-address-town/label"/>
                                <xf:hint ref="$form-resources/uk-address-town/hint"/>
                                <xf:alert ref="$fr-resources/detail/labels/alert"/>


                            </xf:input>
                        </fr:c>
                        <fr:c x="1" y="2" w="6">
                            <xf:input id="uk-address-line-2-control" bind="uk-address-line-2-bind">
                                <xf:label ref="$form-resources/uk-address-line-2/label"/>
                                <xf:hint ref="$form-resources/uk-address-line-2/hint"/>
                                <xf:alert ref="$fr-resources/detail/labels/alert"/>


                            </xf:input>
                        </fr:c>
                        <fr:c x="7" y="2" w="3">
                            <xf:input id="uk-address-postcode-control" bind="uk-address-postcode-bind">
                                <xf:label ref="$form-resources/uk-address-postcode/label"/>
                                <xf:hint ref="$form-resources/uk-address-postcode/hint"/>
                                <xf:alert ref="$fr-resources/detail/labels/alert"/>


                            </xf:input>
                        </fr:c>
                        <fr:c x="10" y="2" w="3" h="1"/>
                        <fr:c x="1" y="3" w="6">
                            <xf:input id="uk-address-locality-control" bind="uk-address-locality-bind">
                                <xf:label ref="$form-resources/uk-address-locality/label"/>
                                <xf:hint ref="$form-resources/uk-address-locality/hint"/>
                                <xf:alert ref="$fr-resources/detail/labels/alert"/>


                            </xf:input>
                        </fr:c>
                        <fr:c x="7" y="3" w="6"/>




                    </fr:grid>
                </fr:section>
                <fr:section id="contact-information-section" bind="contact-information-bind">
                    <xf:label ref="$form-resources/contact-information/label"/>
                    <fr:grid id="grid-5-grid" bind="grid-5-bind">
                        <fr:c x="1" y="1" w="4">
                            <xf:input id="contact-information-first-name-control"
                                      bind="contact-information-first-name-bind">
                                <xf:label ref="$form-resources/contact-information-first-name/label"/>
                                <xf:hint ref="$form-resources/contact-information-first-name/hint"/>
                                <xf:alert ref="$fr-resources/detail/labels/alert"/>


                            </xf:input>
                        </fr:c>
                        <fr:c x="5" y="1" w="2" h="1">
                            <xf:input id="contact-information-last-name-control"
                                      bind="contact-information-last-name-bind">
                                <xf:label ref="$form-resources/contact-information-last-name/label"/>
                                <xf:hint ref="$form-resources/contact-information-last-name/hint"/>
                                <xf:alert ref="$fr-resources/detail/labels/alert"/>


                            </xf:input>
                        </fr:c>
                        <fr:c x="7" y="1" w="6">
                            <xf:input id="contact-information-phone-control" bind="contact-information-phone-bind">
                                <xf:label ref="$form-resources/contact-information-phone/label"/>
                                <xf:hint ref="$form-resources/contact-information-phone/hint"/>
                                <xf:alert ref="$fr-resources/detail/labels/alert"/>


                            </xf:input>
                        </fr:c>
                        <fr:c x="1" y="2" w="6">
                            <fr:us-phone xmlns="http://orbeon.org/oxf/xml/form-builder"
                                         xmlns:xxbl="http://orbeon.org/oxf/xml/xbl"
                                         id="contact-information-email-control"
                                         bind="contact-information-email-bind">
                                <xf:label ref="$form-resources/contact-information-email/label"/>
                                <xf:hint ref="$form-resources/contact-information-email/hint"/>
                                <xf:alert ref="$fr-resources/detail/labels/alert"/>


                            </fr:us-phone>
                        </fr:c>
                        <fr:c x="7" y="2" w="6">
                            <xf:input id="control-4-control" bind="control-4-bind">
                                <xf:label ref="$form-resources/control-4/label"/>
                                <xf:hint ref="$form-resources/control-4/hint"/>

                                <xf:alert ref="$fr-resources/detail/labels/alert"/>
                            </xf:input>
                        </fr:c>
                    </fr:grid>
                </fr:section>
            </fr:body>
        </fr:view>
    </xh:body>
</xh:html>