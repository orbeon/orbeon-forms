<xh:html xmlns:xh="http://www.w3.org/1999/xhtml" xmlns:sql="http://orbeon.org/oxf/xml/sql"
         xmlns:fr="http://orbeon.org/oxf/xml/form-runner"
         xmlns:ev="http://www.w3.org/2001/xml-events"
         xmlns:xxf="http://orbeon.org/oxf/xml/xforms"
         xmlns:map="http://www.w3.org/2005/xpath-functions/map"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:array="http://www.w3.org/2005/xpath-functions/array"
         xmlns:math="http://www.w3.org/2005/xpath-functions/math"
         xmlns:exf="http://www.exforms.org/exf/1-0"
         xmlns:saxon="http://saxon.sf.net/"
         xmlns:soap="http://schemas.xmlsoap.org/soap/envelope/"
         xmlns:fb="http://orbeon.org/oxf/xml/form-builder"
         xmlns:xxi="http://orbeon.org/oxf/xml/xinclude"
         xmlns:xi="http://www.w3.org/2001/XInclude"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:xf="http://www.w3.org/2002/xforms">
    <xh:head>
        <xh:title>Springfield PTA Remittance Form</xh:title>
        <xf:model id="fr-form-model" xxf:expose-xpath-types="true" xxf:analysis.calculate="true">


            <xf:instance id="fr-form-instance" xxf:exclude-result-prefixes="#all" xxf:index="id">
                <form>
                    <unit-details>
                        <grid-1>
                            <pta-unit-name/>
                            <unit-number/>
                            <unit-address/>
                        </grid-1>

                    </unit-details>
                    <submission-details>
                        <grid-2>
                            <submitter-name/>
                            <submitter-phone/>
                            <submitter-pta-position/>
                            <submitter-email/>
                            <date-submitted/>
                            <check-number/>
                            <comment/>
                        </grid-2>
                    </submission-details>
                    <dues>
                        <grid-3>
                            <control-4/>
                            <members-count/>
                            <per-capita-dues>4.75</per-capita-dues>
                            <member-dues/>
                            <control-3/>
                            <include-annual-dues>false</include-annual-dues>
                            <annual-dues>125</annual-dues>
                            <annual-council-dues/>
                            <control-9/>
                            <parent-education-donation>0</parent-education-donation>
                            <total-check-amount/>
                        </grid-3>



                    </dues>
                </form>
            </xf:instance>


            <xf:bind id="fr-form-binds" ref="instance('fr-form-instance')">
                <xf:bind id="unit-details-bind" name="unit-details" ref="unit-details">
                    <xf:bind id="grid-1-bind" ref="grid-1" name="grid-1">
                        <xf:bind id="pta-unit-name-bind" name="pta-unit-name" ref="pta-unit-name"
                                 xxf:whitespace="trim"
                                 required="true()"/>
                        <xf:bind id="unit-number-bind" ref="unit-number" name="unit-number"
                                 xxf:whitespace="trim"
                                 required="true()"/>
                        <xf:bind id="unit-address-bind" ref="unit-address" name="unit-address"
                                 xxf:whitespace="trim"
                                 required="true()"/>
                    </xf:bind>

                </xf:bind>
                <xf:bind id="submission-details-bind" ref="submission-details" name="submission-details">
                    <xf:bind id="grid-2-bind" ref="grid-2" name="grid-2">
                        <xf:bind id="submitter-name-bind" ref="submitter-name" name="submitter-name"
                                 xxf:whitespace="trim"
                                 required="true()"/>
                        <xf:bind id="submitter-phone-bind" ref="submitter-phone" name="submitter-phone"
                                 constraint=". = '' or matches(., '^\d{10}$')"
                                 required="true()"/>
                        <xf:bind id="submitter-pta-position-bind" ref="submitter-pta-position"
                                 name="submitter-pta-position"
                                 xxf:whitespace="trim"
                                 required="true()"/>
                        <xf:bind id="submitter-email-bind" ref="submitter-email" name="submitter-email"
                                 type="xf:email"
                                 xxf:whitespace="trim"
                                 required="true()"/>
                        <xf:bind id="date-submitted-bind" ref="date-submitted" name="date-submitted"
                                 type="xs:date"
                                 required="true()"/>
                        <xf:bind id="check-number-bind" ref="check-number" name="check-number"
                                 xxf:whitespace="trim"
                                 type="xs:integer"
                                 required="true()"/>
                        <xf:bind id="comment-bind" ref="comment" name="comment" xxf:whitespace="trim"/>
                    </xf:bind>
                </xf:bind>
                <xf:bind id="dues-bind" ref="dues" name="dues">
                    <xf:bind id="grid-3-bind" ref="grid-3" name="grid-3">
                        <xf:bind id="control-4-bind" ref="control-4" name="control-4"/>
                        <xf:bind id="members-count-bind" ref="members-count" name="members-count"
                                 type="xs:integer"
                                 required="true()"/>
                        <xf:bind id="per-capita-dues-bind" ref="per-capita-dues" name="per-capita-dues"
                                 type="xf:decimal"
                                 readonly="true()"/>
                        <xf:bind id="member-dues-bind" ref="member-dues" name="member-dues" type="xf:decimal"
                                 calculate="$members-count * $per-capita-dues"/>
                        <xf:bind id="control-3-bind" ref="control-3" name="control-3"/>
                        <xf:bind id="include-annual-dues-bind" ref="include-annual-dues"
                                 name="include-annual-dues"
                                 type="xf:boolean"/>
                        <xf:bind id="annual-dues-bind" ref="annual-dues" name="annual-dues" type="xf:decimal"
                                 readonly="true()"/>
                        <xf:bind id="annual-council-dues-bind" ref="annual-council-dues"
                                 name="annual-council-dues"
                                 type="xf:decimal"
                                 constraint="xxf:fraction-digits(2)"
                                 calculate="if ($include-annual-dues = true()) then $annual-dues else 0.0"/>
                        <xf:bind id="control-9-bind" ref="control-9" name="control-9"/>
                        <xf:bind id="parent-education-donation-bind" ref="parent-education-donation"
                                 name="parent-education-donation"
                                 type="xf:decimal"
                                 constraint="xxf:fraction-digits(2)"/>
                        <xf:bind id="total-check-amount-bind" ref="total-check-amount" name="total-check-amount"
                                 type="xf:decimal"
                                 constraint="xxf:fraction-digits(2)"
                                 calculate="$member-dues + $annual-council-dues + $parent-education-donation"/>
                    </xf:bind>



                </xf:bind>
            </xf:bind>


            <xf:instance id="fr-form-metadata" xxf:readonly="true" xxf:exclude-result-prefixes="#all">
                <metadata>
                    <application-name>orbeon</application-name>
                    <form-name>pta-remittance</form-name>
                    <title xml:lang="en">Springfield PTA Remittance Form</title>
                    <description xml:lang="en">This is a remittance form for the Springfield PTA Council.</description>
                    <created-with-version>2022.1-SNAPSHOT PE</created-with-version>
                    <updated-with-version>2022.1-SNAPSHOT PE</updated-with-version>
                    <data-migration>enabled</data-migration>
                </metadata>
            </xf:instance>


            <xf:instance id="fr-form-attachments" xxf:exclude-result-prefixes="#all">
                <attachments>
                    <pdf name="" lang="en" filename="PTA Remittance Form.pdf"
                         mediatype="application/pdf"
                         size="78447">/fr/service/persistence/crud/orbeon/builder/data/21f5689171e2204b75d8e54becf0c6fa9ce8ac85/4ccffb5e691d8b2ec4c29f9bea88feb2e625580c.bin</pdf>

                </attachments>
            </xf:instance>


            <xf:instance xxf:readonly="true" id="fr-form-resources" xxf:exclude-result-prefixes="#all">
                <resources>
                    <resource xml:lang="en">
                        <submitter-name>
                            <label>Submitted by</label>
                            <hint/>
                        </submitter-name>
                        <submitter-phone>
                            <label>Phone</label>
                            <hint/>
                        </submitter-phone>
                        <submitter-pta-position>
                            <label>Position</label>
                            <hint/>
                        </submitter-pta-position>
                        <submitter-email>
                            <label>Email</label>
                            <hint/>
                        </submitter-email>
                        <date-submitted>
                            <label>Date Submitted</label>
                            <hint/>
                        </date-submitted>
                        <check-number>
                            <label>Check #</label>
                            <hint/>
                        </check-number>
                        <comment>
                            <label>Comment</label>
                            <hint/>
                        </comment>
                        <control-4>
                            <text xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
                                  xmlns:xbl="http://www.w3.org/ns/xbl"
                                  xmlns:xxbl="http://orbeon.org/oxf/xml/xbl"
                                  xmlns:p="http://www.orbeon.com/oxf/pipeline"
                                  xmlns:oxf="http://www.orbeon.com/oxf/processors">&lt;div&gt;Per Capita Dues (due monthly)&lt;/div&gt;</text>
                        </control-4>
                        <members-count>
                            <label>Members Count</label>
                            <hint/>
                        </members-count>
                        <per-capita-dues>
                            <label>Fee per Member</label>
                            <hint/>
                        </per-capita-dues>
                        <member-dues>
                            <label>Member Dues</label>
                            <hint/>
                        </member-dues>
                        <control-3>
                            <text xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
                                  xmlns:xbl="http://www.w3.org/ns/xbl"
                                  xmlns:xxbl="http://orbeon.org/oxf/xml/xbl"
                                  xmlns:p="http://www.orbeon.com/oxf/pipeline"
                                  xmlns:oxf="http://www.orbeon.com/oxf/processors">&lt;div&gt;One time annual Council Dues (due 11/1)&lt;/div&gt;</text>
                        </control-3>
                        <include-annual-dues>
                            <label>Include Dues</label>
                            <hint/>
                        </include-annual-dues>
                        <annual-dues>
                            <label>Annual Dues</label>
                            <hint/>
                        </annual-dues>
                        <annual-council-dues>
                            <label>Annual Council Dues</label>
                            <hint/>
                        </annual-council-dues>
                        <control-9>
                            <text xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
                                  xmlns:xbl="http://www.w3.org/ns/xbl"
                                  xmlns:xxbl="http://orbeon.org/oxf/xml/xbl"
                                  xmlns:p="http://www.orbeon.com/oxf/pipeline"
                                  xmlns:oxf="http://www.orbeon.com/oxf/processors">&lt;div&gt;Donation (optional)&lt;/div&gt;</text>
                        </control-9>
                        <parent-education-donation>
                            <label/>
                            <hint/>
                        </parent-education-donation>
                        <total-check-amount>
                            <label>Total Check Amount</label>
                            <hint/>
                        </total-check-amount>

                        <unit-details>
                            <label>Unit Details</label>
                        </unit-details>
                        <submission-details>
                            <label>Submission Details</label>

                        </submission-details>
                        <dues>
                            <label>Amounts</label>

                        </dues>
                        <pta-unit-name>
                            <label>Name of PTA Unit</label>
                            <hint/>

                        </pta-unit-name>
                        <unit-number>
                            <label>Unit Number</label>
                            <hint/>
                        </unit-number>
                        <unit-address>
                            <label>Address of Unit</label>
                            <hint/>
                        </unit-address>
                    </resource>
                </resources>
            </xf:instance>

        </xf:model>
    </xh:head>
    <xh:body>
